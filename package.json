{
    "name": "server_side_render_with_react_and_express",
    "version": "1.0.0",
    "description": "Server Side Rendering with React and Express",
    "private": true,
    "scripts": {
        "dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/app.js\"",
        "build": "yarn build:client && yarn build:server",
        "build:client": "webpack --config ./webpack/webpack.client.js",
        "build:server": "webpack --config ./webpack/webpack.server.js",
        "clean:restart": "rm -rf dist && rm -rf node_modules && yarn install",
        "start": "yarn build && node dist/server.bundle.js"
    },
    "author": "Cheng Yi Lin",
    "license": "MIT",
    "dependencies": {
        "@babel/runtime": "^7.20.1",
        "dotenv": "^16.0.3",
        "ejs": "^3.1.8",
        "express": "^4.18.2",
        "puppeteer": "^19.2.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "@babel/core": "^7.19.6",
        "@babel/plugin-transform-runtime": "^7.19.6",
        "@babel/preset-env": "^7.19.4",
        "@babel/preset-react": "^7.18.6",
        "@babel/preset-typescript": "^7.18.6",
        "@types/ejs": "^3.1.1",
        "@types/express": "^4.17.14",
        "@types/node": "^18.11.9",
        "@types/react": "^18.0.24",
        "@types/react-dom": "^18.0.8",
        "@types/webpack": "^5.28.0",
        "@typescript-eslint/eslint-plugin": "^5.42.0",
        "@typescript-eslint/parser": "^5.42.0",
        "babel-loader": "^9.0.1",
        "clean-webpack-plugin": "^4.0.0",
        "concurrently": "^7.5.0",
        "copy-webpack-plugin": "^11.0.0",
        "eslint": ">=8.26.0",
        "eslint-config-google": "^0.14.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-react": "^7.31.10",
        "nodemon": "^2.0.20",
        "prettier": "2.7.1",
        "ts-loader": "^9.4.1",
        "tsconfig-paths-webpack-plugin": "^4.0.0",
        "typescript": "^4.8.4",
        "webpack": "^5.74.0",
        "webpack-cli": "^4.10.0",
        "webpack-manifest-plugin": "^5.0.0",
        "webpack-node-externals": "^3.0.0"
    },
    "engines": {
        "node": ">=16.15.0"
    }
}
