{
    "name": "pdf-generation-service",
    "version": "1.0.0",
    "description": "Generate pdf through React SSR",
    "private": true,
    "author": "Cheng Yi Lin",
    "license": "MIT",
    "scripts": {
        "gcp-build": "tsc -p .",
        "prepare": "npm run gcp-build",
