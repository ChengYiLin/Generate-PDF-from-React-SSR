{
    "include": ["../server"],
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "jsx": "react",
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "resolveJsonModule": true,
        "lib": ["es6", "dom"],
        "allowJs": true,
        "strict": true,
        "strictNullChecks": true,
        "noImplicitAny": true
    }
}
